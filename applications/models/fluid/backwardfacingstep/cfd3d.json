{
    "Name": "Fluid Mechanics",
    "ShortName":"Fluid",
    "Model":"Navier-Stokes",
    "Materials":
    {
        "fluid":{
            "name":"myFluidMat",
            "rho":"1",
            // "mu":"0.2"//Re=10
            "mu":"0.02"//Re=100
            // "mu":"0.01"//Re=200
            // "mu":"0.005"//Re=400
        }
    },
    "BoundaryConditions":
    {
        "velocity":
        {
            "Dirichlet":
            {
                "inlet":
                {
                    "expr":"{ 24*y*(1-y)*z*(1-z),0,0 }:y:z"
                },
                "wall":
                {
                    "expr":"{0,0,0}"
                }
            }
        },
        "fluid":
        {
            "outlet":
            {
                "outlet":
                {
                    "expr":"0" 
                }
            }
        }
    },
    "PostProcess":
    {
        "Fields":["velocity","pressure","pid"]
    }

}
