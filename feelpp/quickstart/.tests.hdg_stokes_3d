ethiersteinmann-nosc --config-file ${CMAKE_CURRENT_SOURCE_DIR}/stokes/ethiersteinmann/ethiersteinmann.cfg  --directory=qs_stokes/ethiersteinmann --sc.condense=0 --checker.tolerance.exact=1e-11  --gmsh.hsize=0.2 
