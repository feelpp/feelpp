# SC ONEELT
triangle-sc-oneelt-1 --config-file ${CMAKE_CURRENT_SOURCE_DIR}/cases/triangle/triangle.cfg --sc.condense=1 --checker.tolerance.exact=1e-11 --k=1 --gmsh.hsize=4 --checker.solution=1 --solution.p=1 --solution.u={0,0} 
triangle-sc-oneelt-linear --config-file ${CMAKE_CURRENT_SOURCE_DIR}/cases/triangle/triangle.cfg  --sc.condense=1 --checker.tolerance.exact=1e-11 --k=1 --gmsh.hsize=4 --checker.solution=x+y --solution.p=x+y:x:y --solution.u={1,1}
triangle-sc-oneelt-quadratic --config-file ${CMAKE_CURRENT_SOURCE_DIR}/cases/triangle/triangle.cfg  --sc.condense=1 --checker.tolerance.exact=1e-11 --k=1 --gmsh.hsize=4 --checker.solution=x*x+y*y --solution.p=x*x+y*y:x:y --solution.u={2*x,2*y}:x:y   --functions.f=4 --order=2
# SC
