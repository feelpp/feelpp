// -*- mode: javascript -*-
{
    "Name": "Turek",
    "Model":"Navier-Stokes",
    "Description":"Fluid flow around a cylinder",
    "Parameters":
    {
        "Um":
        {
            "type":"variable",
            "value":0.3,
            "min":1e-4,
            "max":10
        },
        "H":
        {
            "type":"constant",
            "value":0.41
        }
    },
    "Materials":
    {
        "myfluid":
        {
            "markers":["fluid1","fluid2"],
            "physics": "electro",
            "rho": "2", // density
            "eta": "0.001", // dynamic_viscosity
            "mu": "{1,t}:t",
            "nu": "{1,2,x*y}:x:y:z",
            "chi": "{1,2,x,y}:x:y",
            "xhi": "{1,2,3,4,5,6,7,8,t}:t",
            "f" : "2*g:g",
            "h" : "2*g+t:g:t"
        },
	    "mycopper":
        {
            "markers":"copper",
            "physics":["electro","thermo"],
            "rho": "1", // density
            "eta": "0.001", // dynamic_viscosity
            "mu": "{1,t}:t",
            "nu": "{1,2,x*y}:x:y:z",
            "chi": "{1,2,x,y}:x:y",
            "xhi": "{1,2,3,4,5,6,7,8,t}:t",
            "f" : "2*g:g",
            "h" : "2*g+t:g:t"
        }
    },
    "BoundaryConditions":
    {
        "velocity":
        {
            "Dirichlet":
            {
                "inflow":
                {
                    // inflow condition
                    "expr":"{4*Um*y*( H-y )/H^2,0}:y:Um:H"
                },
                /* outflow condition */
                "wall":
                {
                    "expr":"{0,0}"
                },
                "cylinder":
                {
                    "expr":"{0,0}"
                }
            },
            "Neumann":
            {
                "outlet":
                {
                    "expr":"{0,0,0,0}" // outlet
                }
            },
            "Robin":
            {
                "test":
                {
                    "markers":["mark%1%","mymarker","toto%2%","feelpp%1%_p%3%"],
                    "indice1":"1:3",
                    "indice2":"2:8:2",
                    "indice3":["A","D"],
                    "material":"mycopper",
                    "expr1":"{1,2,3}",
                    "expr2":"{x,y,z}:x:y:z"
                }
            }
        }
    }, // BoundaryConditions
    "PostProcess":
    {
        "Force":["cylinder"],
        "PressureDifference":
        {
            "x1":"{0.15,0.2}",
            "x2":"{0.25,0.2}"
        }
    },
    "Outputs":
    {
        "myoutput":
        {
            "type":"average",
            "range":["marker1","marker2"],
            "topodim":3
        },
        "flux":
        {
            "type":"flux",
            "range":"face1",
            "topodim":2
        }
    }
}

