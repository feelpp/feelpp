// -*- mode: javascript -*-
{
    "Name": "Fluid fsi",
    "ShortName":"FluidFSI",
    "Models":
    {
        "fluid":{
            "materials":["Fluid"],
            "equations":"Stokes"
        },
        "solid":{
            "materials":["Solid"],
            "setup":{
                "equations":"Elasticity",
                "body-forces":"{0,-5000}"
            }
        },
        "fsi":{
            "materials":["Fluid","Solid"],
            "setup":{
                "interface":["fsi-wall","contact"]
            }
        }
    },
    "Meshes":
    {
        "fluid":
        {
            "MeshMotion":
            {
                "ComputationalDomain":
                {
                    "markers":"Fluid"
                },
                "Displacement":
                {
                    "Zero":["fluid-inlet","fluid-wall","fluid-cylinder","fluid-outlet"]
                }
            }
        }
    },
    "Materials":
    {
        "Fluid": {
            "rho":"1",
            "mu":"100"
        },
        "Solid": {
            "E":"1.5e8",
            "nu":"0.49",
            "rho":"7.5e3"
        }
    },
    "BoundaryConditions":
    {
        "fluid":
        {
            "velocity":
            {
                "fluid-inlet":
                {
                    "expr":"{0,0}"
                },
                "fluid-wall":
                {
                    "expr":"{0,0}"
                },
                "fluid-cylinder":
                {
                    "expr":"{0,0}"
                },
                "fluid-outlet":
                {
                    "expr":"{0,0}" 
                }
            }
        },
        "solid":
        {
            "displacement":
            {
                "solid-fixed":
                {
                    "expr":"{0,0}"
                }
            }
        }
    },
    "PostProcess":
    {
        "fluid":
        {
            "Exports":
            {
                "fields":["velocity","pressure","mesh-displacement","pid"]
            }
        },
        "solid":
        {
            "Exports":
            {
                "fields":["displacement","velocity","pid"]
            },
            "Measures":
            {
                "Points":
                {
                    "pointA":
                    {
                        "coord":"{2.5,1-0.025,0}",
                        "fields":["displacement","velocity"]
                    }
                }
            }
        }
    }
}