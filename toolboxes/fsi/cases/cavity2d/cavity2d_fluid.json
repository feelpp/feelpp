// -*- mode: javascript -*-
{
    "Name": "Fluid fsi",
    "ShortName":"FluidFSI",
    "Models": {
        "fluid":{
            "equations":"Navier-Stokes"
        }
    },
    "Parameters":
    {
        //"pi":3.1415926,
        //"uxref":"1-cos(2*pi*t/5):t:pi",
        "uxref":"1-cos(2*3.1415926*t/5):t"
    },
    "Materials":
    {
        "Fluid": {
            "rho":"1",
            "mu":"0.01"
        }
    },
    "BoundaryConditions":
    {
        "velocity":
        {
            "Dirichlet":
            {
                "fluid-inlet":
                {
                    "expr":"{ uxref*(8*y-7), 0 }:y:uxref"
                },
                "fluid-noslip-wall":
                {
                    "expr":"{0,0}"
                },
                "fluid-slip-wall":
                {
                    "expr":"{ uxref, 0 }:uxref"
                }
            },
            "interface_fsi":
            {
                "fsi-wall":
                {
                    "expr":"0"
                }
            }
        },
        "fluid":
        {
            "outlet":
            {
                "fluid-outlet":
                {
                    "expr":"0" 
                }
            }
        }
    },
    "PostProcess":
    {
        "fluid":
        {
            "Exports":
            {
                "fields":["velocity","pressure","displacement","pid"]
            }
        }
    }

}
