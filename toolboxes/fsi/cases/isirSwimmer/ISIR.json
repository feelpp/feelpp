// -*- mode: javascript -*-
{
    "Name": "Fluid fsi",
    "ShortName":"FluidFSI",
    "Models":
    {
        "fluid":{
            "materials":["Fluid"],
            "setup":{
            "equations":"Stokes"
            }
        },
        "solid":
            [
                {
                    "name":"magnet",
                    "materials":["Magnet"],
                    "setup":{
                        "equations":"Elasticity",
                        "body-forces":"{0,1e5*sin(3*t)}:t"
                    }
                },
                {
                    "name":"elastic",
                    "materials":["Elastic"],
                    "setup":{
                        "equations":"Elasticity",
                        "body-forces":"{0,1e5*sin(3*t)}:t"
                    }
                }
            ],
        "fsi":{
            "materials":["Fluid","Magnet","Elastic"],
            "setup":{
                "interface":["Tail-boundary","Magnet-boundary"]
            }
        }
    },
    "Meshes":
    {
        "fluid":
        {
            "MeshMotion":
            {
                "ComputationalDomain":
                {
                    "markers":"Fluid"
                    //"method":"harmonic_extension"
                },
                "Displacement":
                {
                    "Zero":["fluid-inlet","fluid-wall","fluid-outlet"]
                }
            }
        }
    },
    "Materials":
    {
        "Fluid": {
            "rho":"1.23e3",
            "mu":"1.524"
        },
        "Magnet": {
            "E":"1.5e11",
            "nu":"0.49",
            "rho":"7.5e3"
        },
        "Elastic": {
                "E":"1.5e8",
                "nu":"0.49",
                "rho":"7.5e3"
        }
    },
    "BoundaryConditions":
    {
        "fluid":
        {
            "velocity":
            {
                "fluid-inlet":
                {
                    "expr":"{0,0}"
                },
                "fluid-wall":
                {
                    "expr":"{0,0}"
                }
            },
            "outlet":
            {
                "fluid-outlet":
                {
                    "expr":"0"
                }
            }
        },
        "solid":
        {
        }
    },
    "PostProcess":
    {
        "fluid":
        {
            "Exports":
            {
                "fields":["velocity","pressure","mesh-displacement","pid"]
            }
        },
        "solid":
        {
            "Exports":
            {
                "fields":["displacement","velocity","pid"]
            }
        }
    }

}
