{
    "Name": "Square2d",
    "ShortName": "Square2d",
    "Models":
    {
        "use-model-name": 1,
        "fluid":
        {
            "equations":"Navier-Stokes"
        },
        "levelset1":
        {
            "equations":"Advection"
        }
    },
    "Materials":
    {
        "OmegaFluid":
        {
            "rho":1,
            "mu":1
        }
    },
    "Parameters":
    {
        "x0": "0.",
        "y0": "0.",
        "r0": "0.2",
        "ubar":"0.2"
    },
    "InitialConditions":
    {
        "multifluid.levelset1":
        {
            "Expression":
            {
                "ic1":
                {
                    "expr":"sqrt( (x-x0)*(x-x0) + (y-y0)*(y-y0) ) - r0 :x:x0:y:y0:r0"
                }
            }
        }
    },
    "BoundaryConditions":
    {
        "velocity":
        {
            "Dirichlet":
            {
                "Left":
                {
                    "expr":"{ ubar*y, 0 }:ubar:y"
                },
                "Right":
                {
                    "expr":"{ ubar*y, 0 }:ubar:y"
                },
                "Top":
                {
                    "expr":"{ubar,0}:ubar"
                },
                "Bottom":
                {
                    "expr":"{-ubar,0}:ubar"
                }
            }
        }
    },
    "PostProcess":
    {
        "use-model-name": 1,
        "multifluid":
        {
            "Exports":
            {
                "fields":["fluid.velocity","fluid.pressure","fluid.density", "fluid.viscosity", "levelset1.phi", "pid"]
            }
        },
        "fluid":
        {
        },
        "levelset1":
        {
        }
    }

}
