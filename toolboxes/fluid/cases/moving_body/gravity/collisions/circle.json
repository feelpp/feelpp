{
    "Name": "Falling circle",
    "ShortName":"Falling circle",
    "Models":
    {
        "fluid":
        {
            "materials":"Fluid",
            "setup":{
                "equations":"Navier-Stokes",
                "gravity":
                {
                    "enable":1,
                    "expr":"{0,-981}:t" 
                }
            }
        },
        "body":
        {
            "materials":["Cir"]
        }
    },
    "Meshes":
    {
        "fluid":
        {
            "Import":
            {
                "filename":"$cfgdir/circle.geo",
                "hsize":0.01
            },
            "MeshMotion":
            {
                "ComputationalDomain":
                {
                    "markers":"Fluid",
                    "method":"harmonic_extension"
                },
                "Displacement":
                {
                    "Zero":["BoxWalls"]
                }
            }
        }
    },
    "Materials":
    {
        "Fluid":{
            "markers":"Fluid",
            "rho":"1",
            "mu":"0.01"
        },
        "Cir":{
            "rho":"1.25"
        }
    },
    "BoundaryConditions":
    {
        "fluid":
        {
            "velocity":
            {
                "BoxWalls":
                {
                    "expr":"{0,0}"
                }
            },

            "body":
            {
                "Circle":
                {
                    "markers":"Circle",
                    "materials":
                    {
                        "names":["Cir"]
                    }
                }
            }
        }
    },
    "CollisionForce":  
    {
        "body":
        {
            "setup": 
            {
                "model":"contactAvoidance",
                "type":"sphericalShapedBody", 
                "forceParam":
                {
                    "forceRange":0.03, 
                    "epsBody":0.001,  
                    "epsWall":0.000001,
                    "exportDistance":0
                }
            }
        }
    },
    "PostProcess":
    {
        "fluid":{
            "Exports":
            {
                "fields":["velocity","pressure","pid","displacement"]
            },
            "Measures":
            {
                "Quantities":
                {
                    "names":"all"
                }
            }
        }
       
    }

}