                    "expr":"{ 1.5*ubar*23.795*(4./0.1681)*y*(0.41-y)*z*(0.41-z)*( ((1-cos(pi*t/2))/2)*(t<2) + (1-(t<2)) ),0,0}:ubar:y:z:t"
                },
                "wall":
                {
                    "markers":["wall1","wall2"],
                    "expr":"{0,0,0}"
                }
            }
        },
        "fluid":
        {
            "outlet":
            {
                "outlet":
                {
                    "expr":"0" 
                }
            }
        }
    },
    "PostProcess":
    {
        "Exports":
        {
            "fields":["velocity","pressure","pid"]
        },
        "Measures":
        {
            "Forces":"wall2"
        }
    }

}
