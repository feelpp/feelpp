{
    "Name": "Fluid Mechanics",
    "ShortName":"Fluid",
    "Models":
    {
        "equations":"Navier-Stokes"
    },
    "Parameters":
    {
        "ubar":"2",
        "chi":"t<2:t"
    },
    "Materials":
    {
        "Fluid":{
            "rho":"1.0e3",
            "mu":"1.0"
        }
    },
    "BoundaryConditions":
    {
        "velocity":
        {
            "Dirichlet":
            {
                "inlet":
